func max(a, b, c) {
    if (a >= b and a >= c) {
        return a;
    } else if (b >= a and b >= c) {
        return b;
    } else {
        return c;
    }
}

func add(x, y) {
    return x + y;
}

func power(base, exp) {
    if (exp == 0) {
        return 1;
    } else {
        return base * power(base, exp - 1);
    }
}

var sum = add(10, add(2, 3) * 2);
print(sum);

var pow_result = power(2 + 1, 3 - 1);
print(pow_result);

var max_val = max(7, 15 - 3, add(5, 8));
print(max_val);